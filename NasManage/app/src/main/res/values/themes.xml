<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="Theme" parent="@android:style/Theme.Holo.Light.DarkActionBar">
        <item name="android:textViewStyle">@style/theme_textview_style</item>
    </style>

    <style name="theme_textview_style" parent="@android:style/Widget.Holo.Light.TextView">
        <item name="android:textColor">@color/pink</item>
        <item name="android:textColorPrimary">@color/fh</item>
    </style>
    <!-- Base application theme. -->
    <style name="Theme.NasManage" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->

        <item name="colorPrimary">@color/fh</item>
        <item name="colorPrimaryVariant">#000000</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/fh</item>
        <item name="statusBarForeground">@color/exo_white</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.NasManage.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.NasManage.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.NasManage.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>